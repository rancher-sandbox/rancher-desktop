    "@electron/notarize": "3.1.1",
    "@eslint/compat": "2.0.0",
    "@playwright/test": "1.57.0",
    "@types/cross-spawn": "6.0.6",
    "@types/dompurify": "3.2.0",
    "@types/ejs": "3.1.5",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.21",
    "@types/mustache": "4.2.6",
    "@types/node": "22.19.3",
    "@types/node-forge": "1.3.14",
    "@types/plist": "3.0.5",
    "@types/ps-tree": "1.1.6",
    "@types/semver": "7.7.1",
    "@types/tar-stream": "3.1.4",
    "@types/which": "3.0.4",
    "@vue/cli-plugin-babel": "5.0.9",
    "@vue/cli-plugin-router": "5.0.9",
    "@vue/cli-plugin-vuex": "5.0.9",
    "@vue/cli-service": "5.0.9",
    "@vue/compiler-sfc": "3.5.25",
    "@vue/eslint-config-standard-with-typescript": "9.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@yarnpkg/cli": "^4.12.0",
    "@yarnpkg/core": "^4.5.0",
    "@yarnpkg/types": "^4.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "30.2.0",
    "babel-loader": "10.0.0",
    "cross-env": "10.1.0",
    "css-loader": "7.1.2",
    "ejs": "3.1.10",
    "electron": "39.2.7",
    "electron-builder": "26.3.6",
    "eslint": "9.39.2",
    "eslint-plugin-vue": "10.6.2",
    "extract-zip": "2.0.1",
    "glob": "^13.0.0",
    "globals": "16.5.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "js-yaml-loader": "1.2.2",
    "mustache": "4.2.0",
    "node-addon-api": "8",
    "node-gyp": "12.1.0",
    "node-gyp-build": "4.8.4",
    "node-loader": "^2.1.0",
    "octokit": "5.0.5",
    "plist": "3.1.0",
    "ps-tree": "1.2.0",
    "raw-loader": "4.0.2",
